{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "typescript",
            "tsconfig": "tsconfig.json",
            "problemMatcher": [
                "$tsc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "type": "npm",
            "script": "build",
            "problemMatcher": [
                "$tslint5"
            ]
        },
        {
            "type": "npm",
            "script": "pack",
            "problemMatcher": [
                "$tslint5"
            ]
        },
        {
            "type": "npm",
            "script": "version:patch",
            "problemMatcher": []
        }
    ]
}
